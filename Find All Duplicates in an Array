class Solution {
public:
    vector<int> findDuplicates(vector<int>& nums) {
        map<int,int>m;
        vector<int>ans;
      for(int i:nums){
          m[i]++;
      }
        for(auto x:m){
            if(x.second==2)
                ans.push_back(x.first);
        }
        return ans;
    }
};
